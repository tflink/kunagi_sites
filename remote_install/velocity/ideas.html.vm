<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN">

#parse( "global-variables.vm" )

#header("ideas", "Ideas - $projectname", "List of ideas and feature requests for $projectname", "$projectname, ideas, features")

<div class="section">

    <h2>Ideas</h2>
    <ul>
    #foreach ($issue in $ideas)
        <li><a href="${issue.reference}.html" class="reference">$issue.reference</a> $issue.label</li>
    #end
    </ul>

</div>

#footer()
