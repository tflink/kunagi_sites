<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN">

#parse( "global-variables.vm" )

#header("closedissues", "Closed issues - $projectname", "List of closed issues in $projectname", "$projectname, bugs, issues, features")

<div class="section">

    <h2>Closed issues</h2>
    <ul>
    #foreach ($issue in $closedIssues)
        <li><a href="${issue.reference}.html" class="reference">$issue.reference</a> $issue.label</li>
    #end
    </ul>

</div>

#footer()
